module test_nand;
   reg a, b;
   wire s;

   gate_nand nand0(a, b, s);

   initial
     begin
	$dumpfile("and.vcd");
	$dumpvars;
//	$display("\ttime,\tb,\tb, \ts");
	$monitor("%b\t%b\t%b", a, b, s);

	a = 0;
	b = 0;
	#5;
	a = 1;
	b = 0;
	#5;
	a = 0;
	b = 1;
	#5;
	a = 1;
	b = 1;

     end
endmodule // test_nand
